{
	"name": "Darkio",
	"type": "dark",
	"colors": {
		// Normal editor background.
		"editor.background": "#272929",
		"editor.foreground": "#cfcfcf",
		// Activity Badge.
		"activityBarBadge.background": "#2D7F9D",
		"activityBarBadge.foreground": "#cfcfcf",
		// Side bar.
		"sideBarTitle.foreground": "#cfcfcf",
		"sideBar.background": "#222424",
		// Actvity bar.
		"activityBar.activeBorder": "#2D7F9D",
		"activityBar.background": "#272929",
		"activityBar.activeBackground": "#272929",
		"activityBar.foreground": "#cfcfcf",
		// Badge background.
		"badge.background": "#2D7F9D",
		"badge.foreground": "#cfcfcf",
		// List selection.
		"list.activeSelectionBackground": "#272929",
		"list.inactiveSelectionBackground": "#222424",
		// Title bar.
		"titleBar.activeBackground": "#272929",
		"titleBar.activeForeground": "#cfcfcf",
		// Breadcrumb/tab window. 
		"breadcrumb.background": "#272929",
		"breadcrumb.foreground": "#cfcfcf",
		"breadcrumb.focusForeground": "#cfcfcf",
		"breadcrumb.activeSelectionForeground": "#cfcfcf",
		// Breadcrumbpicker.
		"breadcrumbPicker.background": "#272929",
		// Dropdown.
		"dropdown.background": "#272929",
		// Dropdowns in settings.
		"settings.dropdownBackground": "#272929",
		// Checkboxes.
		"checkbox.background": "#2D7F9D",
		"checkbox.border": "#2D7F9D",
		// Statusbar background.
		"statusBar.background": "#222424",
		// Folder tree view.
		"tree.indentGuidesStroke": "#444747",
		// Tab.
		"tab.activeBackground": "#272929",
		"tab.inactiveBackground": "#222424",
		// GroupHeader.
		"editorGroupHeader.tabsBackground": "#222424",
		// Scrollbar.
		"scrollbarSlider.background": "#2d7f9d57",
		"scrollbarSlider.activeBackground": "#2d7f9d57",
		// Hover widget.
		"editorHoverWidget.background": "#222424",
		// Debug Toolbar.
		"debugToolBar.background": "#2e2e2e",
		// Search results highlighter.
		"editor.findMatchBackground": "#222424",
		// Search results matches highlighter.
		"editor.findMatchHighlightBackground": "#222424",
		// Selection background.
		"editor.selectionBackground": "#383b3b",
		// Indent Lines in editor.
		"editorIndentGuide.background": "#444747",
		// Suggestion window.
		"editorSuggestWidget.background": "#222424",
		"editorSuggestWidget.foreground": "#cfcfcf",
		"editorSuggestWidget.selectedBackground": "#272929",
		// Icons color.
		"icon.foreground": "#cfcfcf",
		// Menu Background.
		"menu.background": "#222424",
		"menu.foreground": "#cfcfcf",
		"menu.selectionBackground": "#2d7f9d57",
		"menu.separatorBackground": "#444747",
		// Settings.
	},
	"semanticHighlighting": true,
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#515555"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#cac68f"
			},
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#c4c7c7"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#b34444"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#c478bd"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#9861b8"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#8dccf1"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#9d69c5"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#61b4d4"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#9e6060"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#71be6e"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#c4c7c7"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#85d385"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#a8a8a8"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#cea07a"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#c0a866"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#80c27a"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#d8d8d8"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#b4b4b4"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#dad893"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5293dd"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#d2d883"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#d2d883"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#497eb9"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#ff6767"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#d89657"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#386496"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#79d879"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "italic underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#1c3450"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": [
				"source.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#b95050"
			}
		},
